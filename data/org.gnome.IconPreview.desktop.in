[Desktop Entry]
Name=Icon Preview
Comment=Preview icons
Exec=gnome-icon-preview %f
# Traslators: Don't translate this, it's the application icon name
Icon=org.gnome.IconPreview
Terminal=false
Type=Application
StartupNotify=true
Categories=Utility;GTK;GNOME;
DBusActivatable=true
Actions=palette;
MimeType=image/svg+xml;

[Desktop Action palette]
Name=Colour Palette
Exec=gnome-icon-preview --palette

